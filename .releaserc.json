{
  "branches": [
    "+([0-9])?(.{+([0-9]),x}).x",
    "main",
    "next",
    "next-major",
    {
      "name": "alpha",
      "prerelease": true,
      "channel": "alpha"
    },
    {
      "name": "beta",
      "prerelease": true,
      "channel": "beta"
    }
  ],
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "releaseRules": [
          {
            "type": ":sparkles: feat",
            "release": "minor"
          },
          {
            "type": ":bug: fix",
            "release": "patch"
          },
          {
            "type": ":zap: perf",
            "release": "patch"
          },
          {
            "type": ":ambulance: hotfix",
            "release": "patch"
          },
          {
            "type": ":lipstick: style",
            "release": "patch"
          },
          {
            "type": ":ambulance: hotfix",
            "release": "patch"
          },
        ]
      }
    ],
    "@semantic-release/release-notes-generator",
    "@semantic-release/npm",
    "@semantic-release/github"
  ]
}